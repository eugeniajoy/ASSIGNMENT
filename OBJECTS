class Student:
    def __init__(self):
        self.total = none
        self.x = int(input("please enter cat marks"))
        self.y =  int(input("please enter final marks"))
        print(f'cat: {self.x} final_exam: {self.y}')

    def  grades(self):
        self.total = self.x + self.y
        print(f'the total is : {self.total} ')

    def qualify(self):
        if self.total > 45
            print(f"your total score is {self.total}"
